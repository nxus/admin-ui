{
  "name": "@nxus/admin-ui",
  "version": "2.3.6",
  "description": "An Admin UI Framework for Nxus Apps",
  "main": "lib",
  "scripts": {
    "test": "NODE_ENV=test mocha --compilers js:babel/register -R spec",
    "compile": "babel --optional runtime -d lib/ src/",
    "prepublish": "npm run compile",
    "postpublish": "npm run docs-readme && npm run commit-update-docs",
    "docs-readme": "export NAME=`npm view . name`; export VERSION=`npm view . version`; documentation readme ./src/*.js --name $NAME --project-version $VERSION --readme-file ./README.md -s $NAME",
    "commit-update-docs": "git add ./README.md && git commit -m 'Updated README API Docs' && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nxus/admin-ui.git"
  },
  "keywords": [
    "nxus",
    "admin",
    "admin-ui"
  ],
  "author": "mjreich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nxus/admin-ui/issues"
  },
  "homepage": "https://github.com/nxus/admin-ui#readme",
  "peerDependencies": {
    "@nxus/core": "^2.3.0",
    "@nxus/renderer": "^2.3.0",
    "@nxus/templater": "^2.3.0",
    "@nxus/router": "^2.3.0",
    "@nxus/storage": "^2.3.0"
  },
  "devDependencies": {
    "babel": "^5.6.23",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "documentation": "^4.0.0-beta",
    "mocha": "^2.2.5",
    "sinon": "^1.17.2"
  },
  "dependencies": {
    "babel-runtime": "^5.7.0",
    "bluebird": "^3.2.1",
    "morph": "^0.2.0",
    "pluralize": "^1.2.1",
    "underscore": "^1.7.0"
  }
}
